<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <name>Wohninfo</name>
    <description>
        Eine Mieter-Vermieter Kommunikationslösung der HOMEINFO GmbH.
    </description>
    <author email="info@homeinfo.de" href="https://www.homeinfo.de/">
        HOMEINFO - Digitale Informationssysteme GmbH
        Hanomaghof 2
        30449 Hannover
        USt-IdNr. gem. § 27 a UStG: DE 206025922
        Registergericht: Amtsgericht Hannover, Registernummer: HRB 58516
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
